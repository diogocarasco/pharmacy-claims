{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Connect to Delve (Docker)",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "/app",
            "port": 2345,
            "host": "127.0.0.1",
            "showLog": true
        },
        {
            "name": "Launch Pharmacy Claims App",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/main.go",
            "cwd": "${workspaceFolder}",
            "env": {},
            "args": [],
            "showLog": true
        }

    ]
}